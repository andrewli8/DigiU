{
	"name": "DigiU",
	"description": "Track your digital footprint",
	"version": "1.0",
	"permissions": ["history"],
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": ["content.js"]
		}
	],
	"browser_action": {
		"default_icon": "images/my_queen.jpg"
	},
	// default images
	"icons": {
    	"16": "images/get_started16.png",
      	"32": "images/get_started32.png",
      	"48": "images/get_started48.png",
      	"128": "images/get_started128.png"
    },
	"manifest_version": 2
}